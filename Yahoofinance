import yfinance as yf

# Fetch the Nifty50 index data
nifty50 = yf.Ticker("^NSEI")
nifty50_data = nifty50.history()

# Get the last closing price of Nifty50
last_price = nifty50_data['Close'].iloc[-1]

# Round off to nearest 100
rounded_price = round(last_price, -2)

# Remove last 2 characters
formatted_price = str(rounded_price)[:-2]

print(formatted_price)
